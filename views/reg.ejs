<% include ./partials/header %>

<div class="container">
    <h1>Users registered for <%= event.name %></h1>
    <table class="table table-striped table-inverse table-responsive">
        <thead>
            <th>#</th>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Mobile No.</th>
        </thead>
        <tbody>
            <% for(var i=1; i<=event.regs.length; i++){ %>
                <tr>
                    <td><%= i %></td>
                    <td><%= event.regs[i-1].username %></td>
                    <td><%= event.regs[i-1].firstname %></td>
                    <td><%= event.regs[i-1].lastname %></td>
                    <td><%= event.regs[i-1].email %></td>
                    <td><%= event.regs[i-1].mobile %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>

<% include ./partials/footer %>